{
  "name": "styledwindjs",
  "version": "1.0.2",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:client": "tsc -p tsconfig.client.json",
    "build:server": "tsc -p tsconfig.server.json",
    "build:esbuild": "node esbuild.config.js",
    "build": "npm run build:client && npm run build:server && npm run build:esbuild",
    "test": "echo no test"
  },
  "devDependencies": {
    "esbuild": "^0.17.0",
    "typescript": "^4.9.5"
  }
}
