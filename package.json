{
  "name": "styledwindjs",
  "version": "1.0.3",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:client": "tsc -p tsconfig.client.json",
    "build:server": "tsc -p tsconfig.server.json",
    "build:esbuild": "node esbuild.config.js",
    "build:fixdts": "cp dist/client/index.d.ts dist/index.d.ts && cp dist/server/index.d.ts dist/server.d.ts",
    "clean:nonmin": "del-cli \"dist/client/*.js\" \"dist/server/*.js\" \"dist/shared/*.js\"",
    "build": "npm run build:client && npm run build:server && npm run build:esbuild && npm run build:fixdts && npm run clean:nonmin",
    "test": "echo no test"
  },
  "devDependencies": {
    "del-cli": "^3.0.1",
    "esbuild": "^0.17.0",
    "typescript": "^4.9.5"
  }
}
